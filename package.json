{
  "name": "trackmidget",
  "version": "1.0.0",
  "type": "module",
  "description": "URL tracking analytics tool",
  "main": "track.js",
  "scripts": {
    "dev": "vite",
    "dev:css": "npx tailwindcss -i ./src/styles/tailwind.css -o ./src/styles/output.css --watch",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:css\"",
    "build": "vite build && npx tailwindcss -i ./src/styles/tailwind.css -o ./src/styles/output.css --minify",
    "preview": "vite preview"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "chart.js": "^4.4.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "concurrently": "^9.1.2",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "vite": "^6.1.1"
  }
}
